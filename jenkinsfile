pipeline {
  agent any
    stages {
      stage ('Prepare Artifacts') {
        steps {
          sh '''
              zip -r todo.zip *
          '''
          }
        }
     stage ('Upload Artifact') {
        steps {
        sh '''
          curl -f -v -u admin:admin --upload-file todo.zip http://3.236.187.195:8081/repository/todo/todo.zip
         
           ''' 
          }
        }
     }
  }
